local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

local function createGui(scriptUrl1, scriptUrl2)
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "ToshiXGui"
    screenGui.Parent = PlayerGui
    screenGui.ResetOnSpawn = false

    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 350, 0, 220)
    frame.Position = UDim2.new(0.5, -175, 0.5, -110)
    frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    frame.BorderSizePixel = 0
    frame.Parent = screenGui

    local shadow = Instance.new("UIStroke")
    shadow.Thickness = 2
    shadow.Color = Color3.fromRGB(0, 0, 0)
    shadow.Transparency = 0.8
    shadow.Parent = frame

    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 12)
    corner.Parent = frame

    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(0, 200, 0, 40)
    title.Position = UDim2.new(0.5, -100, 0, 10)
    title.Text = "Toshi X"
    title.Font = Enum.Font.GothamBold
    title.TextSize = 24
    title.TextColor3 = Color3.fromRGB(255, 255, 255)
    title.BackgroundTransparency = 1
    title.Parent = frame

    local statusLabel = Instance.new("TextLabel")
    statusLabel.Size = UDim2.new(0, 260, 0, 20)
    statusLabel.Position = UDim2.new(0.5, -130, 0.9, 0)
    statusLabel.Text = ""
    statusLabel.TextColor3 = Color3.fromRGB(255, 80, 80)
    statusLabel.BackgroundTransparency = 1
    statusLabel.Font = Enum.Font.SourceSansBold
    statusLabel.TextScaled = true
    statusLabel.Parent = frame

    -- Botón Sea 1
    local button1 = Instance.new("TextButton")
    button1.Size = UDim2.new(0, 120, 0, 35)
    button1.Position = UDim2.new(0.5, -130, 0.7, 10)
    button1.Text = "Sea 1"
    button1.Font = Enum.Font.GothamBold
    button1.TextSize = 16
    button1.BackgroundColor3 = Color3.fromRGB(0, 140, 220)
    button1.TextColor3 = Color3.fromRGB(255, 255, 255)
    button1.Parent = frame

    local corner1 = Instance.new("UICorner")
    corner1.CornerRadius = UDim.new(0, 8)
    corner1.Parent = button1

    button1.MouseButton1Click:Connect(function()
        statusLabel.Text = "Ejecutando..."
        pcall(function()
            loadstring(game:HttpGet(scriptUrl1, true))()
        end)
        statusLabel.Text = "¡Listo!"
    end)

    -- Botón Sea 2
    local button2 = Instance.new("TextButton")
    button2.Size = UDim2.new(0, 120, 0, 35)
    button2.Position = UDim2.new(0.5, 10, 0.7, 10)
    button2.Text = "Sea 2"
    button2.Font = Enum.Font.GothamBold
    button2.TextSize = 16
    button2.BackgroundColor3 = Color3.fromRGB(0, 140, 220)
    button2.TextColor3 = Color3.fromRGB(255, 255, 255)
    button2.Parent = frame

    local corner2 = Instance.new("UICorner")
    corner2.CornerRadius = UDim.new(0, 8)
    corner2.Parent = button2

    button2.MouseButton1Click:Connect(function()
        statusLabel.Text = "Ejecutando..."
        pcall(function()
            loadstring(game:HttpGet(scriptUrl2, true))()
        end)
        statusLabel.Text = "¡Listo!"
    end)
end

-- URLs de los scripts
local scriptUrl1 = "https://raw.githubusercontent.com/62883/Tsg/refs/heads/main/Toshi%20X%20-%20comunity.lua.txt"
local scriptUrl2 = "https://raw.githubusercontent.com/62883/Sc/refs/heads/main/Sea%202.txt"

createGui(scriptUrl1, scriptUrl2)
