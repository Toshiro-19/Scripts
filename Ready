rSequenceKeypoint.new(0,c.COLORS.DISCORD),ColorSequenceKeypoint.new(1,Color3.fromRGB(68,81,200))}else btnGradient.Color=ColorSequence.new{ColorSequenceKeypoint.new(0,c.COLORS.BUTTON),ColorSequenceKeypoint.new(1,Color3.fromRGB(50,50,75))}end btnGradient.Rotation=45 btnGradient.Parent=btn local emojiLabel=Instance.new("TextLabel")emojiLabel.Name="Emoji"emojiLabel.Size=UDim2.new(1,0,0,20)emojiLabel.Position=UDim2.new(0,0,0,3)emojiLabel.BackgroundTransparency=1 emojiLabel.Text=emoji emojiLabel.TextSize=16 emojiLabel.Parent=btn local nameLabel=Instance.new("TextLabel")nameLabel.Name="Name"nameLabel.Size=UDim2.new(1,-4,0,30)nameLabel.Position=UDim2.new(0,2,0,22)nameLabel.BackgroundTransparency=1 nameLabel.Text=displayName nameLabel.TextColor3=c.COLORS.TEXT_PRIMARY nameLabel.Font=Enum.Font.GothamSemibold nameLabel.TextSize=9 nameLabel.TextWrapped=true nameLabel.TextScaled=false nameLabel.Parent=btn local statusDot=Instance.new("Frame")statusDot.Name="StatusDot"statusDot.Size=UDim2.new(0,6,0,6)statusDot.Position=UDim2.new(1,-10,0,4)statusDot.BackgroundColor3=Color3.fromRGB(100,255,100)statusDot.BorderSizePixel=0 statusDot.Parent=btn local dotCorner=Instance.new("UICorner")dotCorner.CornerRadius=UDim.new(1,0)dotCorner.Parent=statusDot local function onHover()q(btn,{BackgroundColor3=isDiscord and c.COLORS.DISCORD_HOVER or c.COLORS.BUTTON_HOVER})q(btn,{Size=UDim2.new(1,2,1,2)})q(border,{Transparency=0})end local function onLeave()q(btn,{BackgroundColor3=isDiscord and c.COLORS.DISCORD or c.COLORS.BUTTON})q(btn,{Size=UDim2.new(1,0,1,0)})q(border,{Transparency=0.3})end local function onClick()if isDiscord then pcall(function()setclipboard(c.DISCORD_LINK)nameLabel.Text="¬°Copiado!"q(statusDot,{BackgroundColor3=Color3.fromRGB(100,200,255)})wait(1.5)nameLabel.Text=displayName q(statusDot,{BackgroundColor3=Color3.fromRGB(100,255,100)})end)else nameLabel.Text="Cargando..."q(statusDot,{BackgroundColor3=Color3.fromRGB(255,200,100)})local success,error=pcall(function()loadstring(game:HttpGet(c.BASE_URL..scriptFileName))()end)if success then nameLabel.Text="¬°Listo!"q(statusDot,{BackgroundColor3=Color3.fromRGB(100,255,100)})print("‚úÖ "..displayName.." ejecutado correctamente")wait(0.8)s.Enabled=false else nameLabel.Text="Error"q(statusDot,{BackgroundColor3=Color3.fromRGB(255,100,100)})warn("‚ùå Error en "..displayName..": "..tostring(error))wait(2)nameLabel.Text=displayName q(statusDot,{BackgroundColor3=Color3.fromRGB(100,255,100)})end end end btn.MouseEnter:Connect(onHover)btn.MouseLeave:Connect(onLeave)btn.MouseButton1Click:Connect(onClick)return btn end local scripts={{display="DISCORD",file="",emoji="üì°",isDiscord=true,order=1},{display="FISCH",file="Fisch",emoji="üêü",order=2},{display="GROW A GARDEN",file="GrowToshi",emoji="üå±",order=3},{display="REBIRTHS CHAMPIONS",file="RebirthsUltimate",emoji="‚ôªÔ∏è",order=4},{display="COMING SOON",file="",emoji="üéÆ",order=5},{display="COMING SOON",file="",emoji="üéÆ",order=6},{display="COMING SOON",file="",emoji="üéÆ",order=7}}for _,scriptData in ipairs(scripts)do v(scriptData.display,scriptData.file,scriptData.emoji,scriptData.isDiscord or false,scriptData.order)end local dragging=false local dragStart=nil local startPos=nil local function onInputBegan(input)if input.UserInputType==Enum.UserInputType.MouseButton1 or input.UserInputType==Enum.UserInputType.Touch then dragging=true dragStart=input.Position startPos=f.Position end end local function onInputChanged(input)if dragging and(input.UserInputType==Enum.UserInputType.MouseMovement or input.UserInputType==Enum.UserInputType.Touch)then local delta=input.Position-dragStart f.Position=UDim2.new(startPos.X.Scale,startPos.X.Offset+delta.X,startPos.Y.Scale,startPos.Y.Offset+delta.Y)end end local function onInputEnded(input)if input.UserInputType==Enum.UserInputType.MouseButton1 or input.UserInputType==Enum.UserInputType.Touch then dragging=false end end r.InputBegan:Connect(onInputBegan)u.InputChanged:Connect(onInputChanged)u.InputEnded:Connect(onInputEnded)b.MouseButton1Click:Connect(function()q(f,{Size=UDim2.new(0,0,0,0),Position=UDim2.new(0.5,0,0.5,0)},0.3)wait(0.3)s:Destroy()end)f.Size=UDim2.new(0,0,0,0)f.Position=UDim2.new(0.5,0,0.5,0)q(f,{Size=UDim2.new(0,420,0,240),Position=UDim2.new(0.5,-210,0.5,-120)},0.4)s.Enabled=true print("üöÄ Toshix-Scripts Ejecutor v2.0 ¬°Listo!")
