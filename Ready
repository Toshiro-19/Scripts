local p,u,t=game:GetService("Players"),game:GetService("UserInputService"),game:GetService("TweenService")local l=p.LocalPlayer local g=l:WaitForChild("PlayerGui")local c={DISCORD_LINK="https://discord.gg/BvFuNRg5",BASE_URL="https://raw.githubusercontent.com/Toshiro-19/Scripts/refs/heads/main/",ANIMATION_TIME=0.25,COLORS={PRIMARY=Color3.fromRGB(20,20,32),SECONDARY=Color3.fromRGB(30,30,45),ACCENT=Color3.fromRGB(100,255,150),BUTTON=Color3.fromRGB(40,40,60),BUTTON_HOVER=Color3.fromRGB(55,55,80),DISCORD=Color3.fromRGB(88,101,242),DISCORD_HOVER=Color3.fromRGB(108,121,255),TEXT_PRIMARY=Color3.fromRGB(255,255,255),TEXT_SECONDARY=Color3.fromRGB(180,180,180),CLOSE_BUTTON=Color3.fromRGB(255,70,70)}}if g:FindFirstChild("ToshixExecutor")then g.ToshixExecutor:Destroy()end local s=Instance.new("ScreenGui")s.Name="ToshixExecutor"s.Parent=g s.ResetOnSpawn=false s.IgnoreGuiInset=true local f=Instance.new("Frame")f.Name="Shadow"f.Size=UDim2.new(0,420,0,240)f.Position=UDim2.new(0.5,-210,0.5,-120)f.BackgroundColor3=Color3.fromRGB(0,0,0)f.BackgroundTransparency=0.6 f.BorderSizePixel=0 f.Parent=s local h=Instance.new("UICorner")h.CornerRadius=UDim.new(0,10)h.Parent=f local m=Instance.new("Frame")m.Name="MainFrame"m.Size=UDim2.new(1,-4,1,-4)m.Position=UDim2.new(0,2,0,2)m.BackgroundColor3=c.COLORS.PRIMARY m.BorderSizePixel=0 m.Parent=f local n=Instance.new("UICorner")n.CornerRadius=UDim.new(0,8)n.Parent=m local o=Instance.new("UIGradient")o.Color=ColorSequence.new{ColorSequenceKeypoint.new(0,Color3.fromRGB(25,25,40)),ColorSequenceKeypoint.new(1,Color3.fromRGB(15,15,25))}o.Rotation=135 o.Parent=m local r=Instance.new("Frame")r.Name="TitleBar"r.Size=UDim2.new(1,0,0,35)r.BackgroundColor3=c.COLORS.SECONDARY r.BorderSizePixel=0 r.Parent=m local i=Instance.new("UICorner")i.CornerRadius=UDim.new(0,8)i.Parent=r local a=Instance.new("TextLabel")a.Name="Title"a.Size=UDim2.new(1,-70,1,0)a.Position=UDim2.new(0,10,0,0)a.BackgroundTransparency=1 a.Text="üöÄ Toshix-Scripts v2.0"a.TextColor3=c.COLORS.ACC